project 'Snapchat.xcodeproj'

# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'Snapchat' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Snapchat
  pod 'Firebase/Core', '3.16.0'
  pod 'Firebase/Auth', '3.16.0'
  pod 'Firebase/Storage', '3.16.0'
  pod 'Firebase/Database', '3.16.0'
  pod 'SDWebImage', '~>3.8'

end
